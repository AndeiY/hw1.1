a = int(input("Введите число от -1000 до 1000"))
print("The next number for the namber", a, "is", str(a+1) +".")
print("The previous number for the namber", a, "is", str(a-1) +".")


1.2
n = int(input())
k = int(input())
a = k // n
b = k - (a * n)
print(a)
print(b)

1.3
v = int(input())
t = int(input())
result = 0
if v > 0:
    result = (v * t)
else:
    result = (-v * t) % 109
    result = 109 - result
print(result % 109)

